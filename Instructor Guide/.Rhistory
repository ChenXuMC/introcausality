df <- tibble(z = rnorm(300),
gamma = c(rep(-1,100),rep(0,100),rep(1,100))) %>%
mutate(x = gamma*z + rnorm(300)) %>%
mutate(y = 1.5*x+rnorm(300))
help("iv_robust")
summary(iv_robust(y~x|z,data=df))
summary(iv_robust(y~x|z,data=df,weights=gamma))
summary(iv_robust(y~x|z,data=df,subset=gamma==1))
